<Application x:Class="Plantjes.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Plantjes"
             StartupUri="Views/Home/LoginWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="Password">./Font/password.ttf#Password</FontFamily>
        <SolidColorBrush x:Key="base-color">#EBFADC</SolidColorBrush>
        <SolidColorBrush x:Key="secondary-color">#CADEB6</SolidColorBrush>
        <SolidColorBrush x:Key="tertiary-color">#3E4239</SolidColorBrush>
    </Application.Resources>
</Application>
